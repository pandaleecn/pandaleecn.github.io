<div class="highlighter-rouge"><div class="highlight"><pre class="highlight"><code>int PartSort()
{

    low = start;
    high = end;

    key = a[end];

    while(low &lt; high)
    {
        while(low &lt; high &amp;&amp; a[low] &lt;= key)
        {
            ++low;
        }

        while(low &lt; high &amp;&amp; a[low] &gt;= key)
        {
            â€”-high;
        }

        if (low &lt; high)
        {
            a[low] &lt;â€”&gt; a[high];
        }
    }

    a[high] &lt;-&gt; a[end];

    retrun low;
}

int findMedian
{
    low = 0;
    high = aLen - 1;
    mid = (aLen - 1) /2

    while(div != mid) {
        if mid &lt; div {
            div = PartSort(a, low, div - 1);
        }
        else
        {
            div = PartSort(a, div+1, high);
        }
    }
    return a[mid];
}
